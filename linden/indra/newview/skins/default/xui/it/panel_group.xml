<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Info sul Gruppo" name="GroupInfo" title="Informazioni sul gruppo">
	<panel.string name="default_needs_apply_text">
		Ci sono modifiche non salvate nella tabella attuale
	</panel.string>
	<panel.string name="want_apply_text">
		Vuoi salvare le modifiche?
	</panel.string>
	<line_editor label="Scrivi qui il nuovo nome del tuo gruppo" name="group_name_editor"/>
	<texture_picker name="insignia" tool_tip="Clicca per scegliere una foto"/>
	<accordion name="groups_accordion">
		<accordion_tab name="tab_general" title="Generale"/>
		<accordion_tab name="tab_roles" title="Ruoli"/>
		<accordion_tab name="tab_notices" title="Notice"/>
		<accordion_tab name="tab_land" title="Terra/Beni patrimoniali"/>
	</accordion>
	<button label="Annulla" label_selected="Annulla" name="btn_cancel"/>
	<button label="Salva" label_selected="Salva" name="btn_ok"/>
	<button label="Aggiorna" label_selected="Aggiorna" name="btn_refresh"/>
	<button label="Iscriviti (L$0)" label_selected="Iscriviti (L$0)" name="join_button"/>
	<button label="Dettagli" label_selected="Visuale dettagliata" name="info_button"/>
</panel>
